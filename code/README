src/
├── data/
│   ├── ApplicantList.csv       // Contains applicant data (Name, NRIC, Age, Marital Status, Password)
│   ├── OfficerList.csv         // Contains HDB officer data
│   ├── ManagerList.csv         // Contains manager data
│   ├── BTOProjectList.csv      // Contains BTO project data
├── models/
│   ├── Applicant.java
│   ├── HDBOfficer.java
│   ├── Manager.java
│   ├── BTOProject.java
│   ├── Enquiry.java
│   ├── Application.java
│   └── enums/
│       ├── FlatType.java
│       ├── ApplicationStatus.java
│       ├── RegistrationStatus.java
│       └── MaritalStatus.java
├── controllers/
│   ├── AuthController.java
│   ├── ApplicationController.java
│   ├── EnquiryController.java
│   ├── ProjectController.java
│   ├── ManagerController.java
│   └── OfficerRegistrationController.java
├── utils/
│   ├── DataLoader.java         // Handles CSV file reading
│   ├── FileUtils.java          // General file-handling utilities
│   └── PasswordUtils.java      // Handles password security features
├── views/
│   └── ConsoleUI.java          // For CLI interactions
└── tests/
    ├── AuthControllerTest.java
    ├── ApplicationControllerTest.java
    ├── ProjectControllerTest.java
